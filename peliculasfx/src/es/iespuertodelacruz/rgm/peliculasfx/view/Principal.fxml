<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="343.0" prefWidth="501.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="es.iespuertodelacruz.rgm.peliculasfx.controller.PrincipalController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <ButtonBar layoutX="15.0" layoutY="268.0" prefHeight="40.0" prefWidth="464.0">
        <buttons>
            <Button fx:id="btnListado" mnemonicParsing="false" onAction="#listado" text="Listado" />
          <Button fx:id="btnEstadisticas" mnemonicParsing="false" onAction="#estadisticas" prefHeight="24.0" prefWidth="402.0" text="Estadisticas" />
        </buttons>
      </ButtonBar>
      <ImageView fx:id="imgView" fitHeight="236.0" fitWidth="444.0" layoutX="41.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Marvel_logo.jpg" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
